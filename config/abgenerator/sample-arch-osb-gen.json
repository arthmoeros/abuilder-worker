{
    "generateSampleArchOSB1": {
        "rootContents": [
            {
                "folder": {
                    "name": "osb_project",
                    "targetTmplName": "OSB_&{(aup)bizDomain}_&{(aup)bizEntity}",
                    "contents": [
                        {
                            "abtmpl": {
                                "name": "project_file.abtmpl",
                                "targetname": ".project"
                            }
                        },
                        {
                            "abtmpl": {
                                "name": "proxy_service.proxy.abtmpl",
                                "targetTmplName": "PX_&{(sixPaddLeftZero)serviceId}_&{serviceName}&{(prefixUnderscore)serviceType}_&{(aup)serviceVersion}.proxy"
                            }
                        },
                        {
                            "folder": {
                                "name": "settings_folder",
                                "targetname": ".settings",
                                "contents": [
                                    {
                                        "abtmpl": {
                                            "name": "org.eclipse.wst.common.component.abtmpl",
                                            "targetname": "org.eclipse.wst.common.component"
                                        }
                                    },
                                    {
                                        "static": {
                                            "name": "org.eclipse.wst.common.project.facet.core.xml.static",
                                            "targetname": "org.eclipse.wst.common.project.facet.core.xml"
                                        }
                                    },
                                    {
                                        "static": {
                                            "name": "org.eclipse.wst.validation.prefs.static",
                                            "targetname": "org.eclipse.wst.validation.prefs"
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "folder": {
                                "includeif": "&{additionalFolders}",
                                "name": "resources_folder",
                                "targetname": "Resources"
                            }
                        }
                    ]
                }
            }
        ]
    }
}