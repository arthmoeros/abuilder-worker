  "dependencies": {
    "@qsdt/common": "^2.0.3",
    "@qsdt/core-storage-fs": "0.0.3",
    "@qsdt/template-engine": "^2.0.11",
    "@types/fs-extra": "^2.1.0",
    "@types/shelljs": "^0.7.5",
    "@types/uuid": "^2.0.30",
    "folder-zip-sync": "^1.0.5",
    "fs-extra": "^3.0.1",
