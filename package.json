{
  "name": "@artifacter/core",
  "version": "2.1.1",
  "description": "Artifacter's core module (artifact generation manager)",
  "scripts": {
    "prepublish": "tsc",
    "build": "tsc -p .",
    "build:watch": "tsc -p . -w",
    "dev": "npm run build && concurrently \"npm run build:watch\" \"node ./src/start-server.js\""
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arthmoeros/artifacter-core.git"
  },
  "keywords": [
    "artifacter",
    "artifact",
    "generation",
    "worker"
  ],
  "author": "arthmoeros",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/arthmoeros/artifacter-core/issues"
  },
  "homepage": "https://github.com/arthmoeros/artifacter-core#readme",
  "dependencies": {
    "@artifacter/common": "^1.0.5",
    "@artifacter/template-engine": "^2.0.7",
    "@types/fs-extra": "^2.1.0",
    "@types/shelljs": "^0.7.2",
    "@types/uuid": "^2.0.30",
    "folder-zip-sync": "^1.0.4",
    "fs-extra": "^3.0.1",
    "shelljs": "^0.7.8",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
